{
  "name": "wait-time",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "cross-env NODE_ENV=development nodemon ./src/server.js --inspect --exec babel-node\"",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "build": "babel src -d build -s inline --copy-files",
    "prod": "cross-env NODE_ENV=production node ./build/server.js",
    "pretest": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "apollo-engine": "^1.1.0",
    "apollo-server-express": "^1.3.5",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.24.0",
    "moment": "^2.22.2",
    "mssql": "^4.1.0",
    "rollbar": "^2.4.4",
    "slugify": "^1.2.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
